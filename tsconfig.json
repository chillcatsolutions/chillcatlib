{
    "extends": "@tsconfig/recommended/tsconfig.json",
    "compilerOptions": {
        "jsx": "preserve", /* sets the javascript language version */
        "target": "ES2016",
        "module": "commonjs",
        "skipLibCheck": true, /* skip type checking all .d.ts files */
        "noUncheckedIndexedAccess": true,
        "noEmit": true
    }
}